<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Background Colors-->
    <Color x:Key="primaryBackColor1">#414141</Color>
    <Color x:Key="primaryBackColor2">#373737</Color>
    <Color x:Key="primaryBackColor3">#2D2D2D</Color>
    <Color x:Key="primaryBackColor4">#232323</Color>

    <!--Window Border Colors-->
    <Color x:Key="winBorderColor1">#202020</Color>

    <!--Button Colors-->
    <SolidColorBrush x:Key="btnControlWindowColor" Color="#464646"/>
    <SolidColorBrush x:Key="btnControlWindowColorIsMouseOver" Color="#787878"/>

    <!--Panel Colors-->
    <SolidColorBrush x:Key="panelColor" Color="#E30528"/>
    <SolidColorBrush x:Key="panelOverColor" Color="#545454"/>
    <SolidColorBrush x:Key="panelActiveColor" Color="#202020"/>

    <!--Font Colors-->
    <SolidColorBrush x:Key="titleColor1" Color="#C8C8C8"/>
    <SolidColorBrush x:Key="plainTextColor1" Color="#969696"/>

    <!--Color Palette-->
    <SolidColorBrush x:Key="color1" Color="#E285DE"/>
    <SolidColorBrush x:Key="color2" Color="#784DFD"/>
    <SolidColorBrush x:Key="color3" Color="#4ADAEC"/>
    <SolidColorBrush x:Key="color4" Color="#FB539B"/>
    <SolidColorBrush x:Key="color5" Color="#7E82FC"/>
    <SolidColorBrush x:Key="color6" Color="#FFC047"/>
    <SolidColorBrush x:Key="color7" Color="#EF6C96"/>
    <SolidColorBrush x:Key="color8" Color="#78A3FC"/>
    <SolidColorBrush x:Key="color9" Color="#07F3C0"/>
    <SolidColorBrush x:Key="color10" Color="#FBA1AA"/>

</ResourceDictionary>